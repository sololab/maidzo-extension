{
  "name": "__MSG_appName__",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "content_scripts": [
    {
      "css": [
        "bower_components/Bootflat/css/bootstrap.min.css",
        "bower_components/Bootflat/Bootflat/css/bootflat.min.css",
        "styles/main.css"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/Bootflat/js/bootstrap.min.js",
        "bower_components/js-url/url.min.js",
        "scripts/config.js",
        "scripts/main.js"
      ],
      "matches": [
        "http://item.taobao.com/*",
        "https://item.taobao.com/*",
        "http://world.taobao.com/item/*",
        "https://world.taobao.com/item/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://world.tmall.com/*",
        "https://world.tmall.com/*",
        "http://*.yao.95095.com/*",
        "https://*.yao.95095.com/*",
        "http://*.1688.com/*",
        "https://*.1688.com/*"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "cookies",
    "debugger",
    "history",
    "idle",
    "management",
    "notifications",
    "pageCapture",
    "tabs",
    "topSites",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Maidzo",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/icon-48.png",
    "order_panel.html"
  ]
}